# insert: space, tuple, flag
to_hex(req.RequestInsert(mock, 1,   ["hello", "world"], 0x01)._bytes)
#0D00000018000000000000000100000001000000020000000568656C6C6F05776F726C64
to_hex(req.RequestInsert(mock, 2,   ["1", "hello", "wor"], 0x02)._bytes)
#0D000000180000000000000002000000020000000300000001310568656C6C6F03776F72
to_hex(req.RequestInsert(mock, 255, ["2", "hel", "w"], 0x04)._bytes)
#0D0000001400000000000000FF000000040000000300000001320368656C0177

# select: space, index, tuples, offset, limit
to_hex(req.RequestSelect(mock, 1,   255, [[]], 0, 0)._bytes)
#11000000180000000000000001000000FF00000000000000000000000100000000000000
to_hex(req.RequestSelect(mock, 2,   2,   [["1", "hello"], ["wor"]], 10, 10)._bytes)
#11000000280000000000000002000000020000000A0000000A000000020000000200000001310568656C6C6F0100000003776F72
to_hex(req.RequestSelect(mock, 255, 1,   [["2", "hel", "w"], []], 100, 100)._bytes)
#110000002400000000000000FF000000010000006400000064000000020000000300000001320368656C017700000000
to_hex(req.RequestSelect(mock, 1,   255, [[]], 150, 200)._bytes)
#11000000180000000000000001000000FF00000096000000C80000000100000000000000
to_hex(req.RequestSelect(mock, 2,   2,   [["1"], ["world", "of"]], 0, 100)._bytes)
#11000000270000000000000002000000020000000000000064000000020000000100000001310200000005776F726C64026F66
to_hex(req.RequestSelect(mock, 255, 1,   [["1"], ["2"], ["3"], ["hi", "man"], ["good"]], 100, 0)._bytes)
#110000003A00000000000000FF0000000100000064000000000000000500000001000000013101000000013201000000013302000000026869036D616E0100000004676F6F64

# delete: space, tuple, flags
to_hex(req.RequestDelete(mock, 1,   ["hello", "world"], 0x01)._bytes)
#150000001F00000000000000010000000100000001000000125B2768656C6C6F272C2027776F726C64275D
to_hex(req.RequestDelete(mock, 2,   ["1", "hello", "wor"], 0x02)._bytes)
#150000002200000000000000020000000100000001000000155B2731272C202768656C6C6F272C2027776F72275D
to_hex(req.RequestDelete(mock, 255, ["2", "hel", "w"], 0x04)._bytes)
#150000001E00000000000000FF0000000100000001000000115B2732272C202768656C272C202777275D

# update: space, key, op_list(), flags
to_hex(req.RequestUpdate(mock, 1,   "hello", [(1, "+", 2), (0, "=", 3)], 0x01)._bytes)
#130000002A000000000000000100000001000000010000000568656C6C6F020000000100000001040200000000000000000403000000
to_hex(req.RequestUpdate(mock, 2,   "1", [(2, "^", 3)], 0x02)._bytes)
#130000001C0000000000000002000000010000000100000001310100000002000000030403000000
to_hex(req.RequestUpdate(mock, 255, "", [], 0x04)._bytes)
#130000001100000000000000FF00000001000000010000000000000000

# call: proc, args, flag
to_hex(req.RequestCall(mock, "mumba",   [], 0)._bytes)
#160000000E0000000000000000000000056D756D626100000000
to_hex(req.RequestCall(mock, "mamba",   ["1", "hello", "wor"], 0)._bytes)
#160000001A0000000000000000000000056D616D62610300000001310568656C6C6F03776F72
to_hex(req.RequestCall(mock, "caramba", ["2", "hel", "w", "size", "fize", "kize", "loice"], 0)._bytes)
#160000002D000000000000000000000007636172616D62610700000001320368656C01770473697A650466697A65046B697A65056C6F696365

# ping:
to_hex(req.RequestPing(mock)._bytes)
#00FF00000000000000000000
